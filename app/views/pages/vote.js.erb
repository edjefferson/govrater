<% unless @no_vote %>
$("#results").html("<%= escape_javascript(render 'votes') %>");

country_id = parseInt($("#vote1").attr("data-govrecordid"))

if (!Cookies.get("lvc")) {
  Cookies.set("lvc",0)
}
Cookies.set("lvc",Cookies.get("lvc") + 1)
if (!Array.isArray(Cookies.get("cvc"))) {
  Cookies.set("cvc",[])
}
a = Cookies.get("cvc")
if (!a.includes(country_id)){
  a.push(country_id)
  a = [...new Set(a)]
  Cookies.set("cvc",a)
}
<% end %>